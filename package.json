{
  "name": "auto-auto-dashboard",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron/electron.js",
  "homepage": "./",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@google-cloud/vision": "^2.3.3",
    "@types/classnames": "^2.2.10",
    "@types/node": "^13.13.4",
    "@types/papaparse": "^5.2.5",
    "@types/react-dom": "^16.9.7",
    "@types/react-fade-in": "^0.1.0",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.19.2",
    "chart.js": "2.7.3",
    "classnames": "^2.3.1",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "firebase": "^8.5.0",
    "get-safe": "^1.5.0",
    "moment": "2.24.0",
    "node-sass": "4.14.1",
    "nouislider": "13.1.1",
    "number-to-words": "^1.2.4",
    "papaparse": "^5.3.0",
    "puppeteer-core": "^10.0.0",
    "puppeteer-in-electron": "^3.0.4",
    "react": "16.8.4",
    "react-chartjs-2": "2.7.4",
    "react-copy-to-clipboard": "5.0.1",
    "react-datetime": "2.16.3",
    "react-dom": "16.8.4",
    "react-fade-in": "^1.0.0",
    "react-google-maps": "9.4.5",
    "react-number-format": "^4.6.4",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.8",
    "react-to-print": "^2.12.6",
    "reactstrap": "^8.9.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "dev": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && yarn install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.40",
    "@electron-forge/maker-deb": "6.0.0-beta.40",
    "@electron-forge/maker-rpm": "6.0.0-beta.40",
    "@electron-forge/maker-squirrel": "6.0.0-beta.40",
    "@electron-forge/maker-zip": "6.0.0-beta.40",
    "concurrently": "^6.1.0",
    "cross-env": "^7.0.3",
    "electron": "^13.1.2",
    "wait-on": "^5.3.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/assets/img/brand/icons/mac/icon.icns",
        "name": "AutoAuto",
        "ignore": [
          "node_modules/react-scripts",
          "node_modules/@fortawesome/fontawesome-free",
          "node_modules/@google-cloud/vision",
          "node_modules/@types",
          "node_modules/axios",
          "node_modules/chart.js",
          "node_modules/classnames",
          "node_modules/moment",
          "node_modules/node-sass",
          "node_modules/nouislider",
          "node_modules/number-to-words",
          "node_modules/papaparse",
          "node_modules/react"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "auto_auto_dashboard"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
